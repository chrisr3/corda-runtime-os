                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.3.xsd">

    <changeSet author="R3.Corda" id="calculator-v1.1">
        <addColumn tableName="calculator">
            <column name="graphing" type="boolean" defaultValue="false">
